namespace SmallObjects.Bond

using DECIMAL = blob;
using DATETIME = int64;
using GUID = blob;

struct Message {
	1: string message;
	2: int32 version;
}

struct Post {
	1: GUID ID;
	2: string title;
	3: bool active;
	4: DATETIME created;
}

struct Complex {
	1: DECIMAL x;
	2: float y;
	3: int64 z;
}
